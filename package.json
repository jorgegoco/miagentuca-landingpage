{
  "name": "miagentuca-landingpage",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "npm run css:build && vite",
    "build": "npm run css:build && vite build",
    "preview": "vite preview",
    "deploy": "npm run build && gh-pages -d dist",
    "css:build": "npx @tailwindcss/cli -i ./src/styles/tailwind.css -o ./public/css/styles.css --minify",
    "css:watch": "npx @tailwindcss/cli -i ./src/styles/tailwind.css -o ./public/css/styles.css --watch"
  },
  "dependencies": {
    "framer-motion": "^12.23.25",
    "lucide-react": "^0.555.0",
    "react": "^19.2.1",
    "react-dom": "^19.2.1"
  },
  "devDependencies": {
    "@tailwindcss/cli": "^4.1.18",
    "@types/node": "^22.14.0",
    "@vitejs/plugin-react": "^5.0.0",
    "autoprefixer": "^10.4.23",
    "gh-pages": "^6.3.0",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.18",
    "typescript": "~5.8.2",
    "vite": "^6.2.0"
  }
}
